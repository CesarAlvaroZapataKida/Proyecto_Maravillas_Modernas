        tools:layout="@layout/fragment_calculadora" />
    <fragment
        android:id="@+id/sonido"
        android:name="com.example.p2.sonido"
        android:label="fragment_sonido"
        tools:layout="@layout/fragment_sonido" />
    <fragment
        android:id="@+id/web"
        android:name="com.example.p2.web"
        android:label="fragment_web"
        tools:layout="@layout/fragment_web" />
    <fragment
        android:id="@+id/app"
        android:name="com.example.p2.app"
        android:label="fragment_app"
        tools:layout="@layout/fragment_app" />
    <fragment
        android:id="@+id/maravillas"
        android:name="com.example.p2.maravillas"
        android:label="fragment_maravillas"
        tools:layout="@layout/fragment_maravillas" />
    <fragment
        android:id="@+id/videopropuesta"
        android:name="com.example.p2.video"
        android:label="fragment_video"
        tools:layout="@layout/fragment_video" />
</navigation>